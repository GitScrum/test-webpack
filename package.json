{
  "name": "webpack-demo",
  "version": "0.0.15",
  "description": "webpack next complex demo",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "eslint --format=node_modules/eslint-formatter-pretty ./{src,test}/**/*.{js,jsx} && nyc ava",
    "testen": "testen -- ava",
    "build": "webpack",
    "watch": "webpack --watch",
    "update": "updtr && ava-codemods --force"
  },
  "testen": {
    "node": [
      "4",
      "5",
      "6"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "xo",
      "xo-react",
      "plugin:xo/recommended",
      "plugin:ava/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "xo",
      "ava",
      "react"
    ]
  },
  "babel": {
    "presets": [
      "es2015-node",
      "react",
      "react-optimize"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "ava-codemods": "^0.2.1",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.14",
    "css-loader": "^0.25.0",
    "eslint": "^3.6.1",
    "eslint-config-xo": "^0.16.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-formatter-pretty": "^1.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-ava": "^3.1.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-xo": "^1.0.0",
    "file-loader": "^0.9.0",
    "jss": "^5.5.5",
    "nyc": "^8.3.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-jss": "^4.1.0",
    "style-loader": "^0.13.1",
    "stylelint-config-standard": "^13.0.2",
    "stylelint-webpack-plugin": "^0.4.0",
    "testen": "^1.8.2",
    "updtr": "^0.2.1",
    "webpack": "^2.1.0-beta.25"
  },
  "dependencies": {}
}
