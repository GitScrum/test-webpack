{
  "name": "webpack-demo",
  "version": "0.0.17",
  "description": "webpack next complex demo",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "eslint ./{src,test}/**/*.{js,jsx} && nyc ava",
    "testen": "testen -- ava",
    "build": "webpack",
    "watch": "webpack --watch",
    "update": "updtr && ava-codemods --force"
  },
  "testen": {
    "node": [
      "4",
      "5",
      "6",
      "7"
    ]
  },
  "eslintConfig": {
    "format": "node_modules/eslint-formatter-pretty",
    "parser": "babel-eslint",
    "extends": [
      "xo",
      "xo-react",
      "plugin:xo/recommended",
      "plugin:ava/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "xo",
      "ava",
      "react"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "react",
      "react-optimize",
      "babili"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "dynamic-import-webpack"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.18.1",
    "ava-codemods": "^0.3.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-babili": "^0.0.11",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.18.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-xo": "^1.0.0",
    "file-loader": "^0.10.0",
    "jss": "^6.1.0",
    "nyc": "^10.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-jss": "^5.1.0",
    "style-loader": "^0.13.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.6.0",
    "testen": "^1.8.2",
    "updtr": "^0.2.3",
    "webpack": "^2.2.0-rc.3"
  },
  "dependencies": {}
}
